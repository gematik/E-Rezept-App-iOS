	<ul>
<% afos.each do |afo_name, details| %>
	<li><a href="#<%= afo_name %>" id="toc-<%= afo_name %>"><%= afo_name %></a></li>
<% end %>
	</ul>
</li>
<% end %>
</ul>
</li>
</ul>
</nav>

<h1>Requirements</h1><% SORTED_SPECS.each do |spec_name, afos| %>

<h2 id="<%= spec_name %>"><%= spec_name %></h2>

<% afos.each do |afo_name, details| %>

<h3 id="<%= afo_name %>"><%= afo_name %>
<a href="https://gemspec.gematik.de/search?<%= afo_name %>">ðŸ”—</a>
</h3>


<% details.each do |details| %>

<% if details['source'] == "intro" %>
<p><%= details['description'] %></p>
<% else %>
<%= "<h4>#{details['part']} </h4>" if details['part'] %>
<p><%= details['description'] %></p>
<details><summary><%= details['file'].sub("../", "") %>:<%= details['line']+13 %><a class="github" href="https://github.com/gematik/E-Rezept-App-iOS/blob/master/<%= details['file'].sub("../", "") %>#L<%= details['line']+13 %>">Github</a></summary>
<pre>
<%= details['code'] %>
</pre>
</details>

<% end %>

<% end %>

<% end %>
<% end %>

